{
  "name": "locapilot",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "lint": "eslint .",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "type-check": "vue-tsc --noEmit",
    "prepare": "husky"
  },
  "dependencies": {
    "@mdi/font": "7.4.47",
    "dayjs": "1.11.19",
    "dexie": "4.2.1",
    "pinia": "3.0.4",
    "primeicons": "7.0.0",
    "primevue": "4.4.1",
    "vue": "^3.5.24",
    "vue-router": "4.6.3",
    "zod": "4.1.13"
  },
  "devDependencies": {
    "@playwright/test": "1.56.1",
    "@types/node": "24.10.1",
    "@typescript-eslint/eslint-plugin": "8.48.0",
    "@typescript-eslint/parser": "8.48.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/browser-playwright": "4.0.13",
    "@vitest/coverage-v8": "4.0.13",
    "@vitest/ui": "4.0.13",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-vue": "10.6.0",
    "fake-indexeddb": "6.2.5",
    "happy-dom": "20.0.10",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "playwright": "^1.56.1",
    "prettier": "3.6.2",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-pwa": "1.1.0",
    "vitest": "4.0.13",
    "vue-tsc": "^3.1.4",
    "workbox-window": "7.4.0"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  }
}
